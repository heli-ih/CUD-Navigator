generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Person {
  id           Int     @id @default(autoincrement())
  name         String
  position     String
  locationTag String?  
  location       Location?    @relation(fields: [locationTag], references: [name])
  photo        String?
}

model Location {
  id   Int    @id @default(autoincrement())
  name String @unique
  persons Person[]
  path Json
}
